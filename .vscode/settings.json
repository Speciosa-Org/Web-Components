{
  "editor.rulers": [
    60,
    80,
    100,
    120
  ],
  "eslint.format.enable": true,
  "eslint.run": "onType",
  "eslint.useESLintClass": true,
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "eslint.options": {
    "overrideConfigFile": "./etc/eslintrc.cjs"
  },
  "stylelint.configFile": "etc/stylelint.config.cjs",
  "stylelint.reportNeedlessDisables": true,
  "stylelint.validate": [
    "css"
  ],
  "indentRainbow.indicatorStyle": "classic",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "json"
  ],
  "shellformat.useEditorConfig": false
}
